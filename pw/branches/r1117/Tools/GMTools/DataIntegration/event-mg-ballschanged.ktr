    <sql>SELECT COALESCE(MAX(stp.gamesession),0), COALESCE(MAX(stp.gamesession),0)
FROM event_mg_ballschanged e
  JOIN event_mg_session s on s.startid = e.mgsessionid AND s.location = e.location
  JOIN sessiontoplayer stp on stp.id = s.sessiontoplayerid AND stp.location = s.location
WHERE e.id = (SELECT max(id) FROM event_mg_ballschanged WHERE location = &apos;${LOCATION}&apos;)
  AND e.location = &apos;${LOCATION}&apos;
</sql>
