<?xml version="1.0" encoding="utf-8"?>
<AppConfig>
  <EmailForErrors>support@email.com</EmailForErrors>
  <ShowEULAOnLaunch>1</ShowEULAOnLaunch>
  <PWSocVersion>0.0.0</PWSocVersion>

  <PwConnectServer>https://pw.nivalnetwork.com:888/pwc</PwConnectServer>
  <PwAccountManual>http://ru.playpw.com/about-game/how-to-play.html</PwAccountManual>

  <ServerStatusUrl>http://launcher.playpw.com/main_auth.html</ServerStatusUrl>
  <PwUrl>http://www.playpw.com/</PwUrl>
  <PwAccountURL>http://account.playpw.com/</PwAccountURL>
  <PwSupportUrl>http://support.playpw.com/</PwSupportUrl>
  <AuthNetworks>fb, ok, vk, pwc, st, ae, gmg</AuthNetworks>
  <OperatorContentUrl>http://launcher.playpw.com/</OperatorContentUrl>
  <TargetUrl>http://cdn.playpw.com/updates/PC/ru-RU/Client/Client.xml</TargetUrl>
  <EULAUrl>http://updates.playpw.com/eula-{info.langCode}.html</EULAUrl>
  <AuthServer>http://pw.nivalnetwork.com:88/</AuthServer>
  <Name>Prime World Trunk Launcher </Name>
  <RefInfoUrl>http://eventlogger.nivalnetwork.com:80/</RefInfoUrl>
  <UseLoginQueue>1</UseLoginQueue>
  <OpenRegTab>True</OpenRegTab>
  <ForcedLanguage>ru-RU</ForcedLanguage>
  <OverrideTutorialFallthrough>False</OverrideTutorialFallthrough>
  <LoginQueueURLPosfix>login</LoginQueueURLPosfix>
  <AuthData><![CDATA[  ]]></AuthData>
  <Authorization>
    <Modes>
      <Steam>
        <CmdArguments>-steam</CmdArguments>
        <ServicesToLogin>st</ServicesToLogin>
        <ServicesForMerge>fb,ok,vk</ServicesForMerge>
      </Steam>
      <Arc>
        <CmdArguments>arc:1</CmdArguments>
        <ServicesToLogin>arc</ServicesToLogin>
          <ServicesForMerge></ServicesForMerge>
      </Arc>
      <Aeria>
        <CmdArguments>-aeria</CmdArguments>
        <ServicesToLogin>ae</ServicesToLogin>
        <ServicesForMerge>fb</ServicesForMerge>
      </Aeria>
      <Site>
          <CmdArguments></CmdArguments>
        <ServicesToLogin>zzma,pwc,vk,fb,ok</ServicesToLogin>
        <ServicesForMerge>zzma,vk,fb,ok</ServicesForMerge>
      </Site>
      <MailRuGC>
        <CmdArguments></CmdArguments>
        <ServicesToLogin>mru</ServicesToLogin>
        <ServicesForMerge></ServicesForMerge>
      </MailRuGC>
      <Gamigo>
        <CmdArguments></CmdArguments>
        <ServicesToLogin>gmg</ServicesToLogin>
        <ServicesForMerge></ServicesForMerge>
      </Gamigo>
      <Draugas>
        <CmdArguments></CmdArguments>
        <ServicesToLogin>dlt</ServicesToLogin>
        <ServicesForMerge></ServicesForMerge>
      </Draugas>
    </Modes>
  </Authorization>
</AppConfig>
