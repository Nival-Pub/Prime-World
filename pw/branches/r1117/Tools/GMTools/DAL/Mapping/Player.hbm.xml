<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DAL"
                   namespace="DAL.Entities">
  <class name="Player">
    <cache usage="read-write" />
    <id name="Id" />
    <property name="Nickname" />
    <property name="Sex" />
    <property name="Nival" />
    <property name="Faction" />
    <property name="Reliability" />
    <property name="Locale" />
    <property name="LeaverCount" />
    <property name="LeaverGMCount" />
    <property name="LeaverPoints" />
    <property name="IsLeaver" />
    <many-to-one name="Guild" />
  </class>
</hibernate-mapping>