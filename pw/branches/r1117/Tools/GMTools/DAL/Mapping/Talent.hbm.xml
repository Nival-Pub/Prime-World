<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DAL"
                   namespace="DAL.Entities">
  <class name="Talent">
    <id name="Id" />
    <many-to-one name="AbilityType" not-null="false" />
    <property name="Name" />
    <property name="Description" length="1023"/>
    <property name="TalentId" length="1024" />
    <property name="DBID" length="1024" />
    <property name="Cooldown" />
    <property name="Tour" />
    <property name="Rarity" />
    <property name="Budget" />
    <property name="Deleted" />
  </class>
</hibernate-mapping>