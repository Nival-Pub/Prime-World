  <step>
    <name>Get SessionToPlayers</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>conn_manage</connection>
    <sql>SELECT
  stp.id
, &apos;${LOCATION}&apos; as location
, stp.playercharacter
, stp.gamesession
, stp.finallevel
, stp.maxlevelreached
, stp.leaver
, stp.faction
, stp.score
, stp.kills
, stp.deaths
, stp.assists
, stp.enemycreepskilled
, stp.neutralcreepskilled
, stp.relayaddress
, stp.guild
FROM sessiontoplayer stp
where stp.id &gt;= ?
  AND id &lt;= ${FINISH_ID}
ORDER BY id</sql>
