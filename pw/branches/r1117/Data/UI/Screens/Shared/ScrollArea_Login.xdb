<?xml version="1.0" encoding="utf-8"?>
<UIScrollAreaLayout>
	<cursors>
		<disabled>ui_disabled</disabled>
		<dragable>ui_dragable</dragable>
		<dragging>ui_dragging</dragging>
		<focused>ui_focused</focused>
		<normal>ui_normal</normal>
		<pushable>ui_pushable</pushable>
		<pushing>ui_pushing</pushing>
	</cursors>
	<cursorTransparent>false</cursorTransparent>
	<defaultEventsCatch>
		<leftDoubleClick>true</leftDoubleClick>
		<leftMouseDown>true</leftMouseDown>
		<leftMouseUp>true</leftMouseUp>
		<mouseMove>true</mouseMove>
		<rightDoubleClick>true</rightDoubleClick>
		<rightMouseDown>true</rightMouseDown>
		<rightMouseUp>true</rightMouseUp>
	</defaultEventsCatch>
	<draggable>false</draggable>
	<enabled>true</enabled>
	<hAlign>Left</hAlign>
	<handleMsgForXScroll></handleMsgForXScroll>
	<handleMsgForYScroll></handleMsgForYScroll>
	<keepAspectRatio>DontCare</keepAspectRatio>
	<location>
		<x>0</x>
		<y>0</y>
	</location>
	<name></name>
	<needCrop>false</needCrop>
	<relatedLayouts />
	<relatedTexts />
	<scriptFile></scriptFile>
	<scrollXLimits>
		<inMax>1</inMax>
		<inMin>0</inMin>
		<outMax>0</outMax>
		<outMin>0</outMin>
	</scrollXLimits>
	<scrollYLimits>
		<inMax>1</inMax>
		<inMin>0</inMin>
		<outMax>0</outMax>
		<outMin>0</outMin>
	</scrollYLimits>
	<size>
		<x>0</x>
		<y>0</y>
	</size>
	<soundActivate>
		<eventName></eventName>
		<groupName></groupName>
		<projectName></projectName>
	</soundActivate>
	<soundHide>
		<eventName></eventName>
		<groupName></groupName>
		<projectName></projectName>
	</soundHide>
	<soundShow>
		<eventName></eventName>
		<groupName></groupName>
		<projectName></projectName>
	</soundShow>
	<spawnMsgOnLimitsChanged></spawnMsgOnLimitsChanged>
	<subclass></subclass>
	<subLayouts />
	<tooltipText textref="" />
	<transparent>false</transparent>
	<vAlign>Top</vAlign>
	<visible>true</visible>
</UIScrollAreaLayout>