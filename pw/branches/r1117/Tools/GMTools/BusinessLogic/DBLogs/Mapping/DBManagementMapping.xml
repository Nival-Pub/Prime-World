<?xml version="1.0" encoding="utf-8" ?>
<ArrayOfTableFormat xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <TableFormat Name="runereplenish">
    <Fields>
      <StorageField Name="player" Type="System.Int64" />
      <StorageField Name="rune" Type="System.Int32" />
      <StorageField Name="timestamp" Type="System.DateTime" />
    </Fields>
  </TableFormat>

  <TableFormat Name="runeunsoulbound">
    <Fields>
      <StorageField Name="player" Type="System.Int64" />
      <StorageField Name="rune" Type="System.Int32" />
      <StorageField Name="talent" Type="System.Int32" />
      <StorageField Name="timestamp" Type="System.DateTime" />
    </Fields>
  </TableFormat>
  
  <TableFormat Name="runeexpire">
    <Fields>
      <StorageField Name="player" Type="System.Int64" />
      <StorageField Name="rune" Type="System.Int32" />
      <StorageField Name="transactionsleft" Type="System.Int32" />
      <StorageField Name="bonusleft" Type="System.Int32" />
      <StorageField Name="timestamp" Type="System.DateTime" />
    </Fields>
  </TableFormat>

  <TableFormat Name="runeheroapply">
    <Fields>
      <StorageField Name="player" Type="System.Int64" />
      <StorageField Name="rune" Type="System.Int32" />
      <StorageField Name="hero" Type="System.String" />
      <StorageField Name="timestamp" Type="System.DateTime" />
    </Fields>
  </TableFormat>

  <TableFormat Name="gwevents">
    <Fields>
      <StorageField Name="mongoId" Type="System.String" />
      <StorageField Name="eventidx" Type="System.Int32" />
      <StorageField Name="enabled" Type="System.Boolean" />
      <StorageField Name="points" Type="System.Int32" />
      <StorageField Name="endtime" Type="System.DateTime" />
      <StorageField Name="limitt" Type="System.Int32" />
    </Fields>
  </TableFormat>

  <TableFormat Name="gweventslog">
    <Fields>
      <StorageField Name="gweventmid" Type="System.String" />
      <StorageField Name="guildid" Type="System.Int64" />
      <StorageField Name="lastpoints" Type="System.Int32" />
      <StorageField Name="currentpoints" Type="System.Int32" />
      <StorageField Name="timestamp" Type="System.DateTime" />
      <StorageField Name="auid" Type="System.Int32" />
    </Fields>
  </TableFormat>

  <TableFormat Name="questeventsstageslog">
    <Fields>
      <StorageField Name="tostagechange" Type="System.Int32" />
      <StorageField Name="auid" Type="System.Int32" />
      <StorageField Name="timestamp" Type="System.DateTime" />
    </Fields>
  </TableFormat>

  <TableFormat Name="questeventsstepslog">
    <Fields>
      <StorageField Name="tostepchange" Type="System.Int32" />
      <StorageField Name="stage" Type="System.Int32" />
      <StorageField Name="auid" Type="System.Int32" />
      <StorageField Name="timestamp" Type="System.DateTime" />
    </Fields>
  </TableFormat>

  <TableFormat Name="runeroll">
    <Fields>
      <StorageField Name="player" Type="System.Int64" />
      <StorageField Name="source" Type="System.String" />
      <StorageField Name="persistentid" Type="System.Int64" />
      <StorageField Name="rune" Type="System.Int32" />
      <StorageField Name="timestamp" Type="System.DateTime" />
      <StorageField Name="runescountold" Type="System.Int32" />
      <StorageField Name="runescountnew" Type="System.Int32" />
    </Fields>
  </TableFormat>

  <TableFormat Name="claim">
    <Fields>
      <StorageField Name="fromplayer" Type="System.Int64" />
      <StorageField Name="toplayer" Type="System.Int64" />
      <StorageField Name="source" Type="System.Int32" />
      <StorageField Name="category" Type="System.Int32" />
      <StorageField Name="locale" Type="System.String" />
      <StorageField Name="comment" Type="System.String" />
      <StorageField Name="chatlog" Type="System.String" />
      <StorageField Name="timestamp" Type="System.DateTime" />
      <StorageField Name="persistentid" Type="System.Int64" />
      <StorageField Name="kills" Type="System.Int32" />
      <StorageField Name="deaths" Type="System.Int32" />
      <StorageField Name="assists" Type="System.Int32" />
      <StorageField Name="points" Type="System.Int32" />
      <StorageField Name="prime" Type="System.Int32" />
      <StorageField Name="distance" Type="System.Int32" />
      <StorageField Name="afks" Type="System.Int32" />
      <StorageField Name="connstatus" Type="System.Int32" />
      <StorageField Name="muid" Type="System.String" />
    </Fields>
  </TableFormat>

  <TableFormat Name="monitoring_results">
    <Fields>
      <StorageField Name="property" Type="System.String" />
      <StorageField Name="counter" Type="System.String" />
      <StorageField Name="value" Type="System.Int32" />
      <StorageField Name="timestamp" Type="System.DateTime" />
    </Fields>
  </TableFormat>

</ArrayOfTableFormat>