  var varname = t_logs + &quot;_finishid&quot;;
  writeToLog(&quot;l&quot;, &quot;SET &quot; + varname + &quot; = &quot; + finishid)
  parentJob.setVariable(varname, finishid);
}
else
{
  writeToLog(&quot;e&quot;, &quot;Parent job wasn&apos;t found&quot;)
}
</jsScript_script>
