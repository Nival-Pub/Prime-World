<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DAL"
                   namespace="DAL.Entities">

  <class name="GWEventLog" table="gweventslog">

    <id name="Id">
      <generator class="sequence">
        <param name="sequence">gweventslog_id_sequence</param>
      </generator>
    </id>

    <property name="GWEventmid" />
    <property name="GuildId" />
    <property name="LastPoints" />
    <property name="CurrentPoints" />
    <property name="TimeStamp" />
    <property name="Auid" />

  </class>
</hibernate-mapping>