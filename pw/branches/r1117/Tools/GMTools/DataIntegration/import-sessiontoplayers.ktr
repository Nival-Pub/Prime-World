  <step>
    <name>ranges</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>conn_analysis</connection>
    <sql>SELECT COALESCE(min(id),0) FROM sessiontoplayer 
WHERE location = &apos;${LOCATION}&apos;
  AND gamesession = (
SELECT min(id) 
FROM gamesession 
WHERE starttime &gt;= (
        SELECT starttime - interval&apos;4hours&apos; 
        FROM gamesession gs JOIN sessiontoplayer stp 
          ON stp.gamesession = gs.id AND stp.location = gs.location
        WHERE stp.id = ${START_ID} AND stp.location = &apos;${LOCATION}&apos; )
  AND timehierarchyid = (
        SELECT to_char(starttime - interval&apos;4hours&apos;, &apos;yyyyMMdd&apos;)::integer
        FROM gamesession gs JOIN sessiontoplayer stp 
          ON stp.gamesession = gs.id AND stp.location = gs.location
        WHERE stp.id = ${START_ID} AND stp.location = &apos;${LOCATION}&apos; )
)
</sql>
