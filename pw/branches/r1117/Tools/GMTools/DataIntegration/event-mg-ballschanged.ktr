  <step>
    <name>SessionToPlayer</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>conn_analysis</connection>
    <sql>SELECT
  gs.id &quot;gsid&quot;,
  stp.id &quot;sessiontoplayerid&quot;,
  pc.player &quot;playerid&quot;
FROM sessiontoplayer stp
  join gamesession gs on stp.gamesession = gs.id and stp.location = gs.location
  join playercharacter pc on pc.id = stp.playercharacter and pc.location = stp.location
WHERE gs.id &gt;= 
  ( SELECT COALESCE(min(gs2.id),0) 
    FROM gamesession gs2
    WHERE gs2.id &lt; ? 
      AND gs2.location = &apos;${LOCATION}&apos;
      AND gs2.starttime &gt;= 
      ( SELECT starttime - interval &apos;2 hours&apos;
        FROM gamesession gs3
        WHERE gs3.id = ? 
          AND gs3.location = &apos;${LOCATION}&apos;) )
  AND gs.location = &apos;${LOCATION}&apos;</sql>
    <limit>0</limit>
    <lookup>max gs.id</lookup>
    <execute_each_row>Y</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
